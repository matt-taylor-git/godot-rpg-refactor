config_version=5

[application]

config/name="Pyrpg-Godot"
config/description="Turn-based RPG refactored to Godot"
config/version="1.0"
run/main_scene="res://scenes/main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="pyrpg-godot"
config/features=PackedStringArray("4.5")

[autoload]

GameManager="*res://scripts/globals/GameManager.gd"
QuestManager="*res://scripts/globals/QuestManager.gd"
DialogueManager="*res://scripts/globals/DialogueManager.gd"
StoryManager="*res://scripts/globals/StoryManager.gd"
CodexManager="*res://scripts/globals/CodexManager.gd"

[display]

window/size/viewport_width=800
window/size/viewport_height=600
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

